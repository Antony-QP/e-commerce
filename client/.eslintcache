>\n          {loading ? (\n            <h4 className='text-danger'>Loading..</h4>\n          ) : (\n            <h4>update category</h4>\n          )}\n          {categoryForm()}\n        </div>\n      </div>\n    </div>\n  );\n};\n\nexport default CategoryUpdate;\n","/Users/antonyparker/Documents/Coding/react_ecommerce/ecoomerce_website/client/src/components/forms/CategoryForm.js",[],"/Users/antonyparker/Documents/Coding/react_ecommerce/ecoomerce_website/client/src/components/forms/LocalSearch.js",[],"/Users/antonyparker/Documents/Coding/react_ecommerce/ecoomerce_website/client/src/pages/admin/sub/SubCreate.js",["123","124","125","126","127"],"/Users/antonyparker/Documents/Coding/react_ecommerce/ecoomerce_website/client/src/actions/sub.js",[],{"ruleId":"128","replacedBy":"129"},{"ruleId":"130","replacedBy":"131"},{"ruleId":"132","severity":1,"message":"133","line":21,"column":6,"nodeType":"134","endLine":21,"endColumn":8,"suggestions":"135"},{"ruleId":"136","severity":1,"message":"137","line":12,"column":10,"nodeType":"138","messageId":"139","endLine":12,"endColumn":14},{"ruleId":"136","severity":1,"message":"140","line":13,"column":10,"nodeType":"138","messageId":"139","endLine":13,"endColumn":22},{"ruleId":"136","severity":1,"message":"141","line":13,"column":24,"nodeType":"138","messageId":"139","endLine":13,"endColumn":38},{"ruleId":"136","severity":1,"message":"142","line":26,"column":9,"nodeType":"138","messageId":"139","endLine":26,"endColumn":17},{"ruleId":"136","severity":1,"message":"143","line":53,"column":9,"nodeType":"138","messageId":"139","endLine":53,"endColumn":21},"no-native-reassign",["144"],"no-negated-in-lhs",["145"],"react-hooks/exhaustive-deps","React Hook useEffect has a missing dependency: 'loadCategory'. Either include it or remove the dependency array.","ArrayExpression",["146"],"no-unused-vars","'Link' is defined but never used.","Identifier","unusedVar","'EditOutlined' is defined but never used.","'DeleteOutlined' is defined but never u
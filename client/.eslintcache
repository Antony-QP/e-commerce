'>{title}</h1>\n        {product && product.ratings && product.ratings.length > 0\n          ? showAverage(product)\n          : <div className=\"text-center\">No Rating Yet</div>}\n        <Card\n          actions={[\n            <Tooltip title={tooltip}>\n            <a onClick={handleAddToCart}>\n              <ShoppingCartOutlined className='text-danger' />\n              <br />\n              Add To Cart\n            </a>\n          </Tooltip>,\n            <Link to='/'>\n              <HeartOutlined className='text-info' />\n              <br />\n              Add To Wishlist\n            </Link>,\n            <RatingModal>\n              <StarRating\n                name={_id}\n                numberOfStars={5}\n                rating={star}\n                isSelectable={true}\n                starRatedColor='red'\n                changeRating={onStarClick}\n              />\n            </RatingModal>,\n          ]}>\n          <ProductListItems product={product} />\n        </Card>\n      </div>\n    </>\n  );\n};\n\nexport default SingleProduct;\n","/Users/antonyparker/Documents/Coding/react_ecommerce/ecoomerce_website/client/src/components/cards/ProductListItems.js",["302","303"],"import React from \"react\";\nimport ProductCard from \"./ProductCard\";\nimport { Link } from \"react-router-dom\";\n\nconst ProductListItems = ({ product }) => {\n  const { price, category, shipping, color, brand, quantity, sold } = product;\n  return (\n    <ul className='list-group'>\n      <li className='list-group-item'>\n        Price\n        <span className='label label-default label-pill pull-xs-right'>\n          ${price}\n        </span>\n      </li>\n\n      {category && (\n        <li className='list-group-item'>\n          Catgeory\n          <Link\n            to={`/category/${category.slug}`}\n            className='label label-default label-pill pull-xs-right'>\n            {category.name}\n          </Link>\n        </li>\n      )}\n\n      <li className='list-group-item'>\n        Shipping{\" \"}\n        <span className='label label-default label-pill pull-xs-right'>\n          {shipping}\n        </span>\n      </li>\n\n      <li className='list-group-item'>\n        Color{\" \"}\n        <span className='label label-default label-pill pull-xs-right'>\n          {color}\n        </span>\n      </li>\n\n      <li className='list-group-item'>\n        Brand{\" \"}\n        <span className='label label-default label-pill pull-xs-right'>\n          {brand}\n        </span>\n      </li>\n\n      <li className='list-group-item'>\n        In Stock{\" \"}\n        <span className='label label-default label-pill pull-xs-right'>\n          {quantity}\n        </span>\n      </li>\n    </ul>\n  );\n};\n\nexport default ProductListItems;\n","/Users/antonyparker/Documents/Coding/react_ecommerce/ecoomerce_website/client/src/actions/rating.js",["304"],"import React from 'react'\nimport StarRating from 'react-star-ratings'\n\nexport const showAverage = (p) => {\n    if(p && p.ratings){\n        let ratingsArray = p && p.ratings\n        let total = [];\n        let length = ratingsArray.length\n        \n        ratingsArray.map((r) => {\n            total.push(r.star)\n        })\n\n        let totalReduced = total.reduce((a, b) => a + b, 0)\n\n        let highest = length * 5;\n\n        let result = (totalReduced * 5 / highest)\n\n        return (\n            <div className=\"text-center pt-1 pb-3\">\n                <span>\n                    <StarRating rating={result} starDimension=\"20px\" starSpacing=\"2px\" starRatedColor=\"red\" editing={false}/>({p.ratings.length})\n                </span>\n            </div>\n        )\n    }\n}","/Users/antonyparker/Documents/Coding/react_ecommerce/ecoomerce_website/client/src/components/modals/RatingModal.js",["305"],"import React, {useState} from \"react\";\nimport { Modal, Button } from \"antd\";\nimport { toast } from \"react-toastify\";\nimport { useSelector } from \"react-redux\
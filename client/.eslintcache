       </Item>\n      )}\n\n      {!user && (\n        <Item key='login' icon={<UserOutlined />} className='float-right'>\n          <Link to='/login'>Login</Link>\n        </Item>\n      )}\n\n      {user && (\n        <SubMenu key='SubMenu' icon={<SettingOutlined />} \n        title={user.name.split('@')[0]} \n        className=\"float-right\">\n          {user && user.role === 'subscriber' &&  <Item><Link to=\"/user/history\">Dashboard</Link></Item>}\n          {user && user.role === 'admin' &&  <Item><Link to=\"/admin/dashboard\">Dashboard</Link></Item>}\n          <Item icon={<LogoutOutlined />} onClick={logout}>\n            Logout\n          </Item>\n        </SubMenu>\n      )}\n\n      <span className=\"float-right p-1\">\n        <Search/>\n      </span>\n    </Menu>\n  );\n};\n\nexport default Header;\n","/Users/antonyparker/Documents/Coding/react_ecommerce/ecoomerce_website/client/src/components/routes/AdminRoute.js",[],"/Users/antonyparker/Documents/Coding/react_ecommerce/ecoomerce_website/client/src/components/routes/UserRoute.js",[],"/Users/antonyparker/Documents/Coding/react_ecommerce/ecoomerce_website/client/src/reducers/userReducer.js",[],"/Users/antonyparker/Documents/Coding/react_ecommerce/ecoomerce_website/client/src/components/nav/UserNav.js",[],"/Users/antonyparker/Documents/Coding/react_ecommerce/ecoomerce_website/client/src/components/routes/LoadingToRedirect.js",[],"/Users/antonyparker/Documents/Coding/react_ecommerce/ecoomerce_website/client/src/components/nav/AdminNav.js",[],"/Users/antonyparker/Documents/Coding/react_ecommerce/ecoomerce_website/client/src/pages/admin/category/CategoryCreate.js",[],"/Users/antonyparker/Documents/Coding/react_ecommerce/ecoomerce_website/client/src/actions/category.js",[],"/Users/antonyparker/Documents/Coding/react_ecommerce/ecoomerce_website/client/src/pages/admin/category/CategoryUpdate.js",["248"],"import React, { useState, useEffect } from \"react\";\nimport AdminNav from \"../../../components/nav/Admin
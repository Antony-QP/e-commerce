en)\n      .then((res) => {\n        console.log(res);\n        setLoading(false);\n        setName(\"\");\n        toast.success(`\"${res.data.name}\" has been updated`);\n        loadSub();\n        history.push('/admin/sub')\n      })\n      .catch((err) => {\n        console.log(err);\n        setLoading(false);\n        if (err.response.status === 400) toast.error(err.response.data);\n      });\n  };\n\n  return (\n    <div className='container-fluid'>\n      <div className='row'>\n        <div className='col-md-2'>\n          <AdminNav />\n        </div>\n        <div className='col'>\n          {loading ? (\n            <h4 className='text-danger'>Loading..</h4>\n          ) : (\n            <h4>Update Sub Category</h4>\n          )}\n\n          <div className='form-group'>\n            <label>Main Category</label>\n            <select\n              name='category'\n              className='form-control'\n              onChange={(e) => setParent(e.target.value)}>\n              <option>Please Select</option>\n              {categories.length > 0 &&\n                categories.map((c) => (\n                  <option key={c._id} value={c._id} selected={c._id === parent}>\n                    {c.name}\n                  </option>\n                ))}\n            </select>\n          </div>\n\n          <CategoryForm\n            handleSubmit={handleSubmit}\n            name={name}\n            setName={setName}\n          />\n        </div>\n      </div>\n    </div>\n  );\n};\n\nexport default SubUpdate;\n","/Users/antonyparker/Documents/Coding/react_ecommerce/ecoomerce_website/client/src/pages/admin/sub/SubCreate.js",[],"/Users/antonyparker/Documents/Coding/react_ecommerce/ecoomerce_website/client/src/actions/sub.js",[],"/Users/antonyparker/Documents/Coding/react_ecommerce/ecoomerce_website/client/src/pages/admin/product/ProductCreate.js",["205"],"import React, { useState, useEffect } from \"react\";\nimport AdminNav from \"../../../components/nav/AdminNav\"
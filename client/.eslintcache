hange = (e) => {\n    setValues({ ...values, [e.target.name]: e.target.value });\n    console.log(e.target.name, \" ------- \", e.target.value);\n  };\n\n  return (\n    <div className='container-fluid'>\n      <div className='row'>\n        <div className='col-md-2'>\n          <AdminNav />\n        </div>\n        <div className='col-md-10'>\n          {loading ? (\n            <LoadingOutlined className='text-danger h1' />\n          ) : (\n            <h4>Product Update</h4>\n          )}\n          <div className='p-3'>\n            <FileUpload\n              values={values}\n              setValues={setValues}\n              setLoading={setLoading}\n            />\n          </div>\n\n          {JSON.stringify(values)}\n          <ProductUpdateForm\n            handleSubmit={handleSubmit}\n            handleOnChange={handleOnChange}\n            categories={categories}\n            values={values}\n          />\n        </div>\n      </div>\n    </div>\n  );\n};\n\nexport default ProductUpdate;\n","/Users/antonyparker/Documents/Coding/react_ecommerce/ecoomerce_website/client/src/components/forms/ProductUpdateForm.js",["264","265"],"import React from \"react\";\n\nconst ProductUpdateForm = ({ handleSubmit, handleOnChange, values, categories }) => {\n  const {\n    title,\n    description,\n    price,\n    category,\n    subs,\n    shipping,\n    quantity,\n    images,\n    colors,\n    brands,\n    brand,\n    color,\n  } = values;\n\n  return (\n    <form onSubmit={handleSubmit}>\n      <div className='form-group'>\n        <label>Title</label>\n        <input\n          type='text'\n          name='title'\n          id=''\n          className='form-control'\n          value={title}\n          onChange={handleOnChange}\n        />\n      </div>\n\n      <div className='form-group'>\n        <label>Description</label>\n        <input\n          type='text'\n          name='description'\n          id=''\n          className='form-control'\n          value={descr